Host *
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
  Compression no

Host github.com
  User git
  ProxyCommand ssh -q -p 29932 pengyu@pengyuzhou.com nc %h %p
  IdentityFile ~/.ssh/id_rsa
  Compression no

Host gitlab.com
  User git
  ProxyCommand ssh -q -p 29932 pengyu@pengyuzhou.com nc %h %p
  IdentityFile ~/.ssh/id_rsa
  Compression no
