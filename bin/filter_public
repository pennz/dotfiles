#!/bin/bash
BRANCH=good-day

git clone --single-branch --branch ${BRANCH} git@gitlab.com:MrCue/shconf.git
pushd shconf
git filter-repo --path .lc --path notes.md --path .config/clash --path .kaggle  --path bin/v2ray_client.json --path bin/v2ray_router_client.json --path .zsh_history --path .bash_history --path .duckdns --invert-paths
git remote add public git@github.com:pennz/dotfiles.git
git fetch public
git rebase public/master
git push --set-upstream public master
popd
